src/components/carousel/index.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/carousel/carousel.tsx",[],[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/carousel/Carousel.tsx",[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/sideMenu/index.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/sideMenu/SideMenu.tsx",[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/sideMenu/mockup.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/productCollection/index.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/productCollection/ProductCollection.tsx",[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/productCollection/ProductImage.tsx",[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/businessPartners/index.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/businessPartners/BusinessPartners.tsx",[],[],"/Users/hung/Documents/GitHub/React-travel/src/views/home/mockups.ts",[],[],[],"/Users/hung/Documents/GitHub/React-travel/src/views/index.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/views/home/index.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/views/home/HomePage.tsx",["237"],[],"import React from \"react\";\nimport styles from \"./HomePage.module.css\";\nimport {\n  Header,\n  Footer,\n  Carousel,\n  SideMenu,\n  ProductCollection,\n  BusinessPartners,\n} from \"../../components\";\n// 導入類似Bootstrap Grid-layout-system\nimport { Row, Col, Typography, Spin } from \"antd\";\nimport sideImage from \"../../assets/images/sider_1.png\";\nimport sideImage2 from \"../../assets/images/sider_2.png\";\nimport sideImage3 from \"../../assets/images/sider_3.png\";\n// 引入語言切換的高階函數\nimport { withTranslation, WithTranslation } from \"react-i18next\";\nimport axios from \"axios\";\nimport { connect } from \"react-redux\";\nimport { RootState } from \"../../redux/store\";\nimport { giveMeDataActionCreator } from \"../../redux/recommendProducts/recommendProductsActions\";\n\nconst mapStateToProps = (state: RootState) => {\n  return {\n    loading: state.recommendProducts.loading,\n    error: state.recommendProducts.error,\n    productList: state.recommendProducts.productList\n  }\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    giveMeData: () => {\n      dispatch(giveMeDataActionCreator());\n    }\n  };\n};\n\ntype PropsType = WithTranslation &\n  ReturnType<typeof mapStateToProps> &\n  ReturnType<typeof mapDispatchToProps>;\n\nclass HomePageComponent extends React.Component<PropsType> {\n  componentDidMount() {\n    this.props.giveMeData();\n  }\n\n  render() {\n    // console.log(this.props.t)\n    const { t, productList, loading, error } = this.props;\n    if (loading) {\n      return (\n        <Spin\n          size=\"large\"\n          style={{\n            margin: \"200 auto\",\n            width: \"100%\",\n          }}\n        />\n      );\n    }\n    if (error) {\n      return <div>網站錯誤：{error}</div>;\n    }\n    return (\n      <div className={styles.App}>\n        <Header />\n        {/* 页面内容 Main-content */}\n        <div className={styles[\"page-content\"]}>\n          {/* Banner */}\n          <Row style={{ margin: 20 }}>\n            <Col span={4}>\n              {/* sidemenu */}\n              <SideMenu />\n            </Col>\n            <Col span={20}>\n              {/* swiper 輪播系統 */}\n              <Carousel />\n            </Col>\n          </Row>\n\n          {/* Product-collection */}\n          <ProductCollection\n            title={\n              <Typography.Title level={3} type=\"warning\">\n                {t(\"home_page.hot_recommended\")}\n              </Typography.Title>\n            }\n            sideImage={sideImage}\n            products={productList[0].touristRoutes}\n          />\n          <ProductCollection\n            title={\n              <Typography.Title level={3} type=\"danger\">\n                {t(\"home_page.best_schedule\")}\n              </Typography.Title>\n            }\n            sideImage={sideImage2}\n            products={productList[1].touristRoutes}\n          />\n          <ProductCollection\n            title={\n              <Typography.Title level={3} type=\"success\">\n                {t(\"home_page.domestic_travel\")}\n              </Typography.Title>\n            }\n            sideImage={sideImage3}\n            products={productList[2].touristRoutes}\n          />\n          <BusinessPartners />\n        </div>\n        <Footer />\n      </div>\n    );\n  }\n}\n\n// 括號語言命名空間 / 組件\nexport const HomePage = connect(\n  mapStateToProps,\n  mapDispatchToProps\n)(withTranslation()(HomePageComponent));\n","/Users/hung/Documents/GitHub/React-travel/src/views/signIn/index.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/views/register/index.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/views/detail/index.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/views/signIn/SignInPage.tsx",[],[],"/Users/hung/Documents/GitHub/React-travel/src/views/register/RegisterPage.tsx",[],[],"/Users/hung/Documents/GitHub/React-travel/src/views/detail/DetailPage.tsx",["238","239","240","241","242"],[],"/Users/hung/Documents/GitHub/React-travel/src/components/header/Header.class.tsx",["243"],[],"import React from \"react\";\nimport styles from \"./Header.module.css\";\nimport logo from \"../../assets/tripadvisor.svg\";\nimport { Layout, Typography, Input, Menu, Button, Dropdown } from \"antd\";\nimport { GlobalOutlined } from \"@ant-design/icons\";\n// 引入HOC高階函數\nimport { withRouter, RouteComponentProps } from \"react-router-dom\";\n// 引入store倉庫\nimport store from \"../../redux/store\";\n// 引入語言reducer\nimport { LanguageState } from \"../../redux/language/languageReducer\";\n// 引入語言切換的高階函數\nimport { withTranslation, WithTranslation } from \"react-i18next\";\n// 導入action集中工廠\nimport { changeLanguageActionCreator } from \"../../redux/language/languageActions\";\nimport { RootState } from \"../../redux/store\";\nimport { connect } from \"react-redux\";\nimport { Dispatch } from \"redux\";\n\nconst mapStateToProps = (state: RootState) => {\n  return {\n    language: state.language.language,\n    languageList: state.language.languageList\n  }\n}\n\nconst mapDispatchToProps = (dispatch: Dispatch) => {\n  return {\n    changeLanguage: (code: \"zh\" | \"en\") => {\n      const action = changeLanguageActionCreator(code);\n      dispatch(action);\n    },\n  };\n};\n\n// 定義props類型\ntype PropsType = RouteComponentProps & // react-router 路由props類型\n  WithTranslation & // i18n props類型\n  ReturnType<typeof mapStateToProps> & // redux store 映射類型\n  ReturnType<typeof mapDispatchToProps>; // redux dispatch 映射類型\n\nclass HeaderComponnet extends React.Component<PropsType> {\n  // 導入click事件與dispatch異位回傳的action\n  // type 類型名稱 playload 任意類型的key 向store dispatch new state\n  menuClickHandler = (e) => {\n    console.log(e);\n    const action = changeLanguageActionCreator(e.key)\n    store.dispatch(action);\n  };\n\n  render() {\n    const { history, t } = this.props;\n    return (\n      <div className={styles[\"app-header\"]}>\n        {/* top-header */}\n        <div className={styles[\"top-header\"]}>\n          <div className={styles.inner}>\n            <Dropdown.Button\n              style={{ marginLeft: 15 }}\n              overlay={\n                /* 加入點擊事件的監聽action */\n                <Menu onClick={this.menuClickHandler}>\n                     {this.props.languageList.map((l) => {\n                    return <Menu.Item key={l.code}>{l.name}</Menu.Item>;\n                  })}\n                </Menu>\n              }\n              icon={<GlobalOutlined />}\n            >\n              {this.props.language === \"zh\" ? \"中文\" : \"English\"}\n            </Dropdown.Button>\n            <Button.Group className={styles[\"button-group\"]}>\n              <Button onClick={() => history.push(\"register\")}>{t(\"header.register\")}</Button>\n              <Button onClick={() => history.push(\"signin\")}>{t(\"header.signin\")}</Button>\n            </Button.Group>\n          </div>\n        </div>\n\n        <Layout.Header className={styles[\"main-header\"]}>\n          <span onClick={() => history.push(\"/\")}>\n            <img src={logo} alt=\"logo\" className={styles[\"App-logo\"]} />\n            <Typography.Title level={4} className={styles.title}>\n            {t(\"header.title\")}\n            </Typography.Title>\n          </span>\n\n          <Input.Search\n            placeholder={\"請輸入旅遊目的地、主题、或關鍵字\"}\n            className={styles[\"search-input\"]}\n          />\n        </Layout.Header>\n        <Menu mode={\"horizontal\"} className={styles[\"main-menu\"]}>\n          <Menu.Item key={1}> {t(\"header.group\")} </Menu.Item>\n          <Menu.Item key={2}> {t(\"header.airplane\")} </Menu.Item>\n          <Menu.Item key={3}> {t(\"header.booking\")} </Menu.Item>\n          <Menu.Item key=\"4\"> {t(\"header.backpack\")} </Menu.Item>\n          <Menu.Item key=\"5\"> {t(\"header.subject\")} </Menu.Item>\n          <Menu.Item key=\"6\"> {t(\"header.cruise\")} </Menu.Item>\n          <Menu.Item key=\"7\"> {t(\"header.hotel\")} </Menu.Item>\n          <Menu.Item key=\"8\"> {t(\"header.local\")} </Menu.Item>\n          <Menu.Item key=\"9\"> {t(\"header.highway\")} </Menu.Item>\n          <Menu.Item key=\"10\"> {t(\"header.ticket\")} </Menu.Item>\n          <Menu.Item key=\"11\"> {t(\"header.visa\")} </Menu.Item>\n        </Menu>\n      </div>\n    );\n  }\n}\n\nexport const Header = connect(mapStateToProps, mapDispatchToProps)(\n  withTranslation()(withRouter(HeaderComponnet))\n);\n","/Users/hung/Documents/GitHub/React-travel/src/redux/store.ts",["244","245","246"],[],"/Users/hung/Documents/GitHub/React-travel/src/i18n/configs.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/redux/language/languageReducer.ts",["247"],[],"// 導入i18n\nimport i18n from \"i18next\";\n// 定義語言的接口 語言代碼\nexport interface LanguageState {\n  language: \"en\" | \"zh\";\n  languageList: { name: string; code: string }[];\n}\n\n//使用接口數據定義 初始state\nconst defaultState: LanguageState = {\n  language: \"zh\",\n  languageList: [\n    { name: \"中文\", code: \"zh\" },\n    { name: \"English\", code: \"en\" },\n  ],\n};\n\n// 處理state引入的舊數據defaultstate透過action處理回傳新的state\n// 使用...展開括號符 用舊的對象創建新的對象\nexport default (state = defaultState, action) => {\n  switch (action.type) {\n    case \"change_language\":\n      i18n.changeLanguage(action.payload); // userffect\n      return { ...state, language: action.payload };\n    default:\n      return state;\n  }\n};\n","/Users/hung/Documents/GitHub/React-travel/src/redux/language/languageActions.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/redux/recommendProducts/recommendProductsReducer.ts",["248"],[],"import {\n  FETCH_RECOMMEND_PRODUCTS_FAIL,\n  FETCH_RECOMMEND_PRODUCTS_START,\n  FETCH_RECOMMEND_PRODUCTS_SUCCESS,\n  RecommendProductAction,\n} from \"./recommendProductsActions\";\n\n// 創建產品推薦的數據接口\ninterface RecommendProductsState {\n  productList: any[];\n  loading: boolean;\n  error: string | null;\n}\n\nconst defaultState: RecommendProductsState = {\n  loading: true,\n  error: null,\n  productList: [],\n};\n\nexport default (state = defaultState, action: RecommendProductAction) => {\n  switch (action.type) {\n    case FETCH_RECOMMEND_PRODUCTS_START:\n      return { ...state, loading: true };\n    case FETCH_RECOMMEND_PRODUCTS_SUCCESS:\n      return { ...state, loading: false, productList: action.payload };\n    case FETCH_RECOMMEND_PRODUCTS_FAIL:\n      return { ...state, loading: false, error: action.payload };\n    default:\n      return state;\n  }\n};\n","/Users/hung/Documents/GitHub/React-travel/src/redux/recommendProducts/recommendProductsActions.ts",[],[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/productIntro/index.ts",[],[],"/Users/hung/Documents/GitHub/React-travel/src/components/productIntro/ProductIn